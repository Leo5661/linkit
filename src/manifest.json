{
    "manifest_version": 3,
    "name": "LinkIt(react)",
    "version": "0.1.1",

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "service_worker": "service.js"
    },
    "action": {
        "default_popup": "index.html",
        "default_title": "LinkIt"
    },
    "permissions": [
        "tabs"
    ],
    "icons": {
        "16": "assets/icon.png"
    }
}